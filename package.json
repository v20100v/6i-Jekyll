{
  "name": "@6i/6i-jekyll-theme",
  "version": "1.0.0",
  "description": "A free and open-source Jekyll theme",
  "homepage": "https://v20100v.github.io/projects/6i-theme-jekyll",
  "repository": "https://github.com/v20100v/6i-jekyll-theme",
  "author": "v20100v <7567933+v20100v@users.noreply.github.com>",
  "license": "Private",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta1",
    "masonry-layout": "^4.2.2",
    "moment": "^2.29.1",
    "swiper": "^6.4.10"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^6.0.4",
    "autoprefixer": "^10.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "gh-pages": "^3.1.0",
    "mini-css-extract-plugin": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "dev": "cross-env-shell NODE_ENV=development JEKYLL_ENV=development npm-run-all --print-label \"dev:clean\" --parallel \"dev:serve\" \"dev:webpack\"",
    "dev:clean": "bundle exec jekyll clean",
    "dev:serve": "bundle exec jekyll serve --open-url",
    "dev:webpack": "webpack --mode development --watch",
    "dev:only-jekyll": "cross-env-shell NODE_ENV=development JEKYLL_ENV=development run-s -l \"dev:clean\" \"dev:serve\"",
    "dev:only-webpack": "cross-env-shell NODE_ENV=development run-s -l \"dev:webpack\"",
    "build": "cross-env-shell NODE_ENV=production JEKYLL_ENV=production run-s -l \"build:webpack\" \"build:jekyll\"",
    "build:jekyll": "cross-env-shell NODE_ENV=production JEKYLL_ENV=production bundle exec jekyll build",
    "build:webpack": "cross-env-shell NODE_ENV=production webpack --mode production",
    "publish": "run-s -l \"build\" \"publish:push-github-pages\"",
    "push-github-pages": "gh-pages -d /_site"
  }
}
